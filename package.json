{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gmail-inbox",
  "title": "Gmail Inbox",
  "description": "Access your Gmail Inbox from Raycast. Review, Send & Delete Emails.",
  "icon": "gmail-icon.png",
  "author": "Reece",
  "categories": [
    "Productivity",
    "Communication",
    "Web",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-gmail-inbox",
      "title": "Open Gmail Inbox",
      "description": "Review, Send & Delete Emails.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "clientId",
      "title": "Client ID:",
      "type": "password",
      "description": "Your ClientId found in Google Dev Console",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.0",
    "del": "^7.0.0",
    "jwt-decode": "^3.1.2",
    "node-fetch": "^3.3.0",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
